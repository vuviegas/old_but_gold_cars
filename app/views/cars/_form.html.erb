<div>
  <%= simple_form_for(@car) do |f| %>
    <div class="form-inputs">
      <%= f.input :brand %>
      <%= f.input :model %>
      <%= f.input :color %>
      <%= f.input :year %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%#= f.input :photo, as: :file %>
    </div>
    <div class="form-actions green-input">
      <%= f.button :submit %>
    </div>
  <% end %>
  </br>
</div>
